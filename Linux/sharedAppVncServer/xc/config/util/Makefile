	  > lndir.$(MANSUFFIX)-html && $(MV) lndir.$(MANSUFFIX)-html $@

install.man:: lndir.$(MANSUFFIX).html
	@if [ -d $(DESTDIR)$(DOCHTMLDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(DOCHTMLDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(DOCHTMLDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(DOCHTMLDIR)); \
	fi
	@(SUF=`expr $(MANSUFFIX) \: '\(.\)'`; \
	 set -x; \
	 $(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) lndir.$(MANSUFFIX).html $(DESTDIR)$(DOCHTMLDIR)/lndir.$$SUF.html)

cleandir::
	$(RM) lndir.$(MANSUFFIX).html lndir.$(MANSUFFIX)-html


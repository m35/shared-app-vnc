xmkmf::	xmkmf.cpp $(ICONFIGFILES)
	$(RM) $@
	 	$(CPP)  -DCONFIGDIRSPEC='"'"-I$(CONFIGDIR)"'"' <xmkmf.cpp | sed -e '/^#  *[0-9][0-9]*  *.*$$/d' 			-e '/^#line  *[0-9][0-9]*  *.*$$/d' 			-e '/^[ 	]*XCOMM$$/s/XCOMM/#/' 			-e '/^[ 	]*XCOMM[^a-zA-Z0-9_]/s/XCOMM/#/' 			-e '/^[ 	]*XHASH/s/XHASH/#/' 			-e '/\@\@$$/s/\@\@$$/\\/' >$@
	chmod a+x $@

cleandir::
	$(RM) xmkmf

ccmakedep::	mdepend.cpp $(ICONFIGFILES)
	$(RM) $@
	 	$(CPP)  -DPREPROC='"'"$(PREPROCESSCMD_MKDEPEND)"'"' <mdepend.cpp | sed -e '/^#  *[0-9][0-9]*  *.*$$/d' 			-e '/^#line  *[0-9][0-9]*  *.*$$/d' 			-e '/^[ 	]*XCOMM$$/s/XCOMM/#/' 			-e '/^[ 	]*XCOMM[^a-zA-Z0-9_]/s/XCOMM/#/' 			-e '/^[ 	]*XHASH/s/XHASH/#/' 			-e '/\@\@$$/s/\@\@$$/\\/' >$@
	chmod a+x $@


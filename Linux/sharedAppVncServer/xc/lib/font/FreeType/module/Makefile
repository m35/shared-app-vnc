all:: libfreetype.a
	@if [ -d $(BUILDMODULEDIR)/fonts ]; then \
		set +x; \
	else \
		if [ -h $(BUILDMODULEDIR)/fonts ]; then \
			(set -x; rm -f $(BUILDMODULEDIR)/fonts); \
		fi; \
		(set -x; $(MKDIRHIER) $(BUILDMODULEDIR)/fonts); \
	fi
	$(RM) $(BUILDMODULEDIR)/fonts/libfreetype.a
	@(UP="`$(REVPATH) fonts`"; set -x; \
	  cd $(BUILDMODULEDIR)/fonts && \
		$(LN) $(BUILDMODULETOP)/$${UP}$(CURRENT_DIR)/libfreetype.a .)

install:: libfreetype.a
	@if [ -d $(DESTDIR)$(MODULEDIR)/fonts ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(MODULEDIR)/fonts ]; then \
			(set -x; rm -f $(DESTDIR)$(MODULEDIR)/fonts); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(MODULEDIR)/fonts); \
	fi
	$(INSTALL) -c $(INSTDATFLAGS) libfreetype.a $(DESTDIR)$(MODULEDIR)/fonts


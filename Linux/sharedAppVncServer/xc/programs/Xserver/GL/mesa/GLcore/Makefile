all:: libGLcore.a
	@if [ -d $(BUILDMODULEDIR)/extensions ]; then \
		set +x; \
	else \
		if [ -h $(BUILDMODULEDIR)/extensions ]; then \
			(set -x; rm -f $(BUILDMODULEDIR)/extensions); \
		fi; \
		(set -x; $(MKDIRHIER) $(BUILDMODULEDIR)/extensions); \
	fi
	$(RM) $(BUILDMODULEDIR)/extensions/libGLcore.a
	@(UP="`$(REVPATH) extensions`"; set -x; \
	  cd $(BUILDMODULEDIR)/extensions && \
		$(LN) $(BUILDMODULETOP)/$${UP}$(CURRENT_DIR)/libGLcore.a .)

install:: libGLcore.a
	@if [ -d $(DESTDIR)$(MODULEDIR)/extensions ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(MODULEDIR)/extensions ]; then \
			(set -x; rm -f $(DESTDIR)$(MODULEDIR)/extensions); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(MODULEDIR)/extensions); \
	fi
	$(INSTALL) -c $(INSTDATFLAGS) libGLcore.a $(DESTDIR)$(MODULEDIR)/extensions


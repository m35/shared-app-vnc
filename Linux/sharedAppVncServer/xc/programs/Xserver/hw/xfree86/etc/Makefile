	@if [ -d $(DESTDIR)$(LIBDIR)/etc ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(LIBDIR)/etc ]; then \
			(set -x; rm -f $(DESTDIR)$(LIBDIR)/etc); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(LIBDIR)/etc); \
	fi
	@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; --*) ;; *[i]*) set +e;; esac; done; \
	for i in $(FILES); do \
		(set -x; $(INSTALL) $(INSTALLFLAGS) $(INSTDATFLAGS) $$i $(DESTDIR)$(LIBDIR)/etc); \
	done

